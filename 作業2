n = int(input('輸入一個2以上的正整數:'))
if(n<2):
    print("錯誤,必須輸入2以上的正整數")
elif n==2:
    print("是質數")
else:
    for i in range(2,n):
        if n % 4 ==0:
            print('是4的倍數')
            break
        if n % i == 0:
            print('不是質數')
            break
        if i == n-1:
            print('是質數')
